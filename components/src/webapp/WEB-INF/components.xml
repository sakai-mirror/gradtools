<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springkernel.org/dtd/spring-beans.dtd">
<beans>
	<bean id="org.sakaiproject.api.app.dissertation.DissertationService"
			class="org.sakaiproject.component.app.dissertation.DbDissertationService"
			init-method="init"
			destroy-method="destroy"
			singleton="true">
		<property name="memoryService"><ref bean="org.sakaiproject.memory.api.MemoryService"/></property>
		<property name="sqlService"><ref bean="org.sakaiproject.db.api.SqlService"/></property>
		<property name="serverConfigurationService"><ref bean="org.sakaiproject.component.api.ServerConfigurationService"/></property>
		<property name="entityManager"><ref bean="org.sakaiproject.entity.api.EntityManager"/></property>
 		<property name="dissertationTableName"><value>DISSERTATION_DISSERTATION</value></property>
 		<property name="stepTableName"><value>DISSERTATION_STEP</value></property>
 		<property name="pathTableName"><value>DISSERTATION_PATH</value></property>
 		<property name="statusTableName"><value>DISSERTATION_STATUS</value></property>
 		<property name="infoTableName"><value>DISSERTATION_CANDIDATEINFO</value></property>
 		<property name="groupTableName"><value>DISSERTATION_GROUP</value></property>
 		<property name="locksInDb"><value>false</value></property>
 		<property name="autoDdl"><value>true</value></property>
 		<property name="stepChangeJob"><ref bean="org.sakaiproject.component.app.dissertation.StepChangeJob"/></property>
 		<property name="uploadExtractsJob"><ref bean="org.sakaiproject.component.app.dissertation.UploadExtractsJob"/></property>
	</bean>
	<bean id="org.sakaiproject.component.app.dissertation.StepChangeJob"
			class="org.sakaiproject.component.app.dissertation.StepChangeJobImpl"
			init-method="init"
			destroy-method="destroy"
			singleton="false">
	</bean>
	<bean id="org.sakaiproject.component.app.dissertation.UploadExtractsJob"
			class="org.sakaiproject.component.app.dissertation.UploadExtractsJobImpl"
			init-method="init"
			destroy-method="destroy"
			singleton="false">
	</bean>
</beans>
